{
  "version": 2,
  "name": "petgram-afisaacs-client",
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {
      "src": "(.*).js", "dest": "/$1.js"
    },
    {
      "src": "(.*).json", "dest": "/$1.json"
    },
    {
      "src": "/.*", "dest":"/index.html"
    },
    {
      "src": "(.*).png", "dest": "/$1.png"
    }
  ]
}
